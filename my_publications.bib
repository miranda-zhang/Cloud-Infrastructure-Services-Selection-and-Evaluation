@inproceedings{cloudcomLiZhengCEEM2013,
abstract = {Although Cloud computing emerged for business applications in industry, public Cloud services have been widely accepted and encouraged for scientific computing in academia. The recently available Google Compute Engine (GCE) is claimed to support high-performance and computationally intensive tasks, while little evaluation studies can be found to reveal GCE's scientific capabilities. Considering that fundamental performance benchmarking is the strategy of early-stage evaluation of new Cloud services, we followed the Cloud Evaluation Experiment Methodology (CEEM) to benchmark GCE and also compare it with Amazon EC2, to help understand the elementary capability of GCE for dealing with scientific problems. The experimental results and analyses show both potential advantages of, and possible threats to applying GCE to scientific computing. For example, compared to Amazon's EC2 service, GCE may better suit applications that require frequent disk operations, while it may not be ready yet for single VM-based parallel computing. Following the same evaluation methodology, different evaluators can replicate and/or supplement this fundamental evaluation of GCE. Based on the fundamental evaluation results, suitable GCE environments can be further established for case studies of solving real science problems.},
author = {Li, Zheng and OBrien, Liam and Ranjan, Rajiv and Zhang, Miranda},
booktitle = {2013 IEEE 5th International Conference on Cloud Computing Technology and Science},
doi = {10.1109/CloudCom.2013.7},
isbn = {978-0-7695-5095-4},
keywords = {Australia,Benchmark testing,CEEM,Cloud Services Evaluation,Cloud computing,GCE scientific capabilities,Google,Google Compute Engine,Google compute engine performance,Measurement,Public Cloud Service,Scientific Computing,Standards,Throughput,benchmark testing,cloud computing,cloud evaluation experiment methodology,computationally intensive tasks,early-stage evaluation,fundamental performance benchmarking,high-performance tasks,public cloud services,scientific computing,scientific information systems,software performance evaluation},
month = {12},
pages = {1--8},
publisher = {IEEE},
shorttitle = {Cloud Computing Technology and Science (CloudCom),},
title = {{Early Observations on Performance of Google Compute Engine for Scientific Computing}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=6753771},
volume = {1},
year = {2013}
}
@article{FGCS_LiZhengCDN2014,
abstract = {Cloud computing is a new paradigm shift which enables applications and related content (audio, video, text, images, etc.) to be provisioned in an on-demand manner and being accessible to anyone anywhere in the world without the need for owning expensive computing and storage infrastructures. Interactive multimedia content-driven applications in the domains of healthcare, aged-care, and education have emerged as one of the new classes of big data applications. This new generation of applications need to support complex content operations including production, deployment, consumption, personalization, and distribution. However, to efficiently provision these applications on the Cloud data centres, there is a need to understand their runtime resource configurations. For example: (i) where to store and distribute the content to and from driven by end-user Service Level Agreements (SLAs)? (ii) How many content distribution servers to provision? And (iii) what Cloud VM configuration (number of instances, types, speed, etc.) to provision? In this paper, we present concepts and factors related to engineering such content-driven applications over public Clouds. Based on these concepts and factors, we propose a performance evaluation methodology for quantifying and understanding the runtime configuration of these classes of applications. Finally, we conduct several benchmark driven experiments for validating the feasibility of the proposed methodology.},
author = {Li, Zheng and Mitra, Karan and Zhang, Miranda and Ranjan, Rajiv and Georgakopoulos, Dimitrios and Zomaya, Albert Y. and O'Brien, Liam and Sun, Shengtao},
doi = {10.1016/j.future.2013.12.019},
issn = {0167739X},
journal = {Future Generation Computer Systems},
keywords = {Application runtime configuration,Cloud services evaluation,Content delivery network,Evaluation methodology,Experimental design and analysis,Mediawise cloud content orchestrator,Public clouds},
month = {7},
pages = {297--308},
title = {{Towards understanding the runtime configuration management of do-it-yourself content delivery network applications over public clouds}},
url = {http://www.sciencedirect.com/science/article/pii/S0167739X13002835},
volume = {37},
year = {2014}
}
